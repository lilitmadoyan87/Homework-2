function getFibonacciNumber(n) {
  let num1=0; 
  let num2=1; 
  let fib = 0; 
  if( n=== 0) {
    return num1;
  } else{
    for (let i = 0; i <= n-1; i++) { 
      fib = num1 + num2; 
      num1 = num2; 
      num2 = fib; 
    }
    return num2; 
  }
}

let n = +prompt('Enter nth number');
console.log(getFibonacciNumber(n));
